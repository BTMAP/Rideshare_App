<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uwi.btmap">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BTMAP">
        <activity
            android:name=".NavActivity"
            android:theme="@style/Theme.BTMAP.NoActionBar"
            />
        <activity android:name=".Activities.MapActivity" />

        <activity android:name=".Activities.UpdateProfileActivity" />
        <activity android:name=".Activities.ProfileActivity" />

        <activity android:name=".Activities.RegisterCommuteActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.BTMAP.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.VerifyActivity"
            android:theme="@style/Theme.BTMAP.NoActionBar" />
        <activity
            android:name=".Activities.PhoneAuthActivity"
            android:theme="@style/Theme.BTMAP.NoActionBar" />
    </application>

</manifest>